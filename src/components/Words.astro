        </div>
        
        <!-- Row 6 -->
        <div class="flex m-0 p-0 justify-center text-center items-center w-full gap-0">
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Recovery Run</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Goal Progress</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Monthly Mileage</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Avg Pace</div>
        </div>
        
        <!-- Row 7 -->
        <div class="flex justify-center text-center items-center w-full gap-0">
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Stride</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Pace</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Splits</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Trail</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Kudos</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Track</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Tempo</div>
        </div>
