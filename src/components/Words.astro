<section class="bg-black py-16 px-6 text-white relative overflow-hidden min-h-screen ">
    <div class="flex flex-col min-h-screen justify-center items-center w-full gap-0">
        <!-- Row 1 -->
        <div class="flex max-h-full m-0 p-0 justify-center text-center items-center w-full gap-0">
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Track</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Strides</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Pace</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Stride</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Tempo</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Kudos</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Trail</div>
        </div>
        
        <!-- Row 2 -->
        <div class="flex m-0 p-0 justify-center text-center items-center w-full gap-0">
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Board</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Log Fuel</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Pace Variance</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Leaderboard</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Create Challenge</div>
        </div>
        
        <!-- Row 3 -->
        <div class="flex m-0 p-0 justify-center text-center items-center w-full gap-0">
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Interval</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Cadence</div>
            <div class="w-full bg-red-900 px-6 py-3 items-center justify-center rounded-full text-sm text-red-200 border border-red-700">Breadcrumbs</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Paceboard</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Runfile</div>
        </div>
        
        <!-- Row 4 - Center row with "Own the Pace" -->
        <div class="flex  m-0 p-0 justify-center text-center items-center w-full gap-0">
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Race Run</div>
            <div class="w-full bg-gray-800 px-6 py-3 items-center justify-center rounded-full text-sm text-gray-300 border border-gray-600">Best Pace</div>
