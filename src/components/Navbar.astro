<script>
    // Cache DOM elements for better performance
    const mobileMenuButton = document.getElementById('mobile-menu-button');
    const mobileMenu = document.getElementById('mobile-menu');
    const featuresButton = document.getElementById('features-menu-button');
    const featuresMenu = featuresButton?.nextElementSibling;
    let isMenuOpen = false;
    let isFeaturesOpen = false;
    
    // Enhanced mobile menu toggle with full accessibility
    function toggleMobileMenu() {
        isMenuOpen = !isMenuOpen;
        mobileMenu?.classList.toggle('hidden');
        mobileMenuButton?.setAttribute('aria-expanded', isMenuOpen.toString());
        mobileMenu?.setAttribute('aria-hidden', (!isMenuOpen).toString());
        
        // Update button label
        const buttonLabel = isMenuOpen ? 'Close navigation menu' : 'Open navigation menu';
        mobileMenuButton?.setAttribute('aria-label', buttonLabel);
        
        // Focus management
        if (isMenuOpen && mobileMenu) {
            const firstMenuItem = mobileMenu.querySelector('[role="menuitem"]') as HTMLElement;
            firstMenuItem?.focus();
        } else if (!isMenuOpen && mobileMenuButton) {
            mobileMenuButton.focus();
        }
        
        // Trap focus within menu when open
        if (isMenuOpen) {
            trapFocus(mobileMenu);
        }
    }
    
    // Enhanced features dropdown accessibility
    function toggleFeaturesMenu(open?: boolean) {
        isFeaturesOpen = open !== undefined ? open : !isFeaturesOpen;
        featuresButton?.setAttribute('aria-expanded', isFeaturesOpen.toString());
        featuresMenu?.setAttribute('aria-hidden', (!isFeaturesOpen).toString());
        
        if (isFeaturesOpen && featuresMenu) {
            const firstMenuItem = featuresMenu.querySelector('[role="menuitem"]') as HTMLElement;
            firstMenuItem?.focus();
            trapFocus(featuresMenu);
        }
    }
    
    // Focus trap utility for accessibility
    function trapFocus(element: Element | null) {
        if (!element) return;
        
        const focusableElements = element.querySelectorAll(
            'a[href], button:not([disabled]), textarea:not([disabled]), input[type="text"]:not([disabled]), input[type="radio"]:not([disabled]), input[type="checkbox"]:not([disabled]), select:not([disabled]), [tabindex]:not([tabindex="-1"])'
        );
        
        const firstElement = focusableElements[0] as HTMLElement;
        const lastElement = focusableElements[focusableElements.length - 1] as HTMLElement;
        
        const handleKeydown = (e: KeyboardEvent) => {
            if (e.key === 'Tab') {
                if (e.shiftKey) {
                    if (document.activeElement === firstElement) {
                        lastElement?.focus();
                        e.preventDefault();
                    }
                } else {
                    if (document.activeElement === lastElement) {
                        firstElement?.focus();
                        e.preventDefault();
                    }
                }
            }
        };
        
        (element as HTMLElement).addEventListener('keydown', handleKeydown);
    }
    
    // Event listeners
    mobileMenuButton?.addEventListener('click', toggleMobileMenu);
    
    // Features dropdown keyboard navigation
    featuresButton?.addEventListener('keydown', (e: KeyboardEvent) => {
        if (e.key === 'Enter' || e.key === ' ') {
            e.preventDefault();
            toggleFeaturesMenu();
        }
    });
    
    // Close menus with Escape
    document.addEventListener('keydown', (e: KeyboardEvent) => {
        if (e.key === 'Escape') {
            if (isMenuOpen) {
                toggleMobileMenu();
            }
            if (isFeaturesOpen) {
                toggleFeaturesMenu(false);
                featuresButton?.focus();
            }
        }
    });
    
    // Enhanced smooth scrolling with performance optimization
    function smoothScrollTo(target: Element) {
        // Use intersection observer for performance
        const observer = new IntersectionObserver((entries) => {
            entries.forEach((entry) => {
                if (entry.isIntersecting) {
                    observer.disconnect();
                }
            });
        });
        
        observer.observe(target);
        
        if ('scrollBehavior' in document.documentElement.style) {
            target.scrollIntoView({ behavior: 'smooth', block: 'start' });
        } else {
            target.scrollIntoView();
        }
    }
    
    // Optimized click handler with analytics
    document.addEventListener('click', function(e: MouseEvent) {
        const target = e.target as HTMLElement;
        if (!target) return;
        
        const link = target.closest('a[href^="#"]') as HTMLAnchorElement | null;
        if (link) {
            const href = link.getAttribute('href');
            if (href && href !== '#' && href.length > 1) {
                e.preventDefault();
                const targetElement = document.querySelector(href);
                if (targetElement) {
                    // Analytics tracking
                    if ('gtag' in window) {
                        (window as any).gtag('event', 'navigation', {
                            'event_category': 'engagement',
                            'event_label': href
                        });
                    }
                    
                    // Close mobile menu if open
                    if (isMenuOpen) {
                        toggleMobileMenu();
                    }
                    
                    // Smooth scroll to target
                    smoothScrollTo(targetElement);
                }
            }
        }
        
        // Close mobile menu when clicking outside
        if (isMenuOpen && target &&
            !mobileMenuButton?.contains(target) && 
            !mobileMenu?.contains(target)) {
            toggleMobileMenu();
        }
    });
    
    // Performance optimizations
    if ('requestIdleCallback' in window) {
        requestIdleCallback(() => {
            // Preload critical resources
            ['//github.com', '//buymeacoffee.com'].forEach(domain => {
                const link = document.createElement('link');
                link.rel = 'dns-prefetch';
                link.href = domain;
                document.head.appendChild(link);
            });
            
            // Preload critical images
            const logoPreload = document.createElement('link');
            logoPreload.rel = 'preload';
            logoPreload.as = 'image';
            logoPreload.href = '/pacebeats-text-white.svg';
            document.head.appendChild(logoPreload);
        });
    }
</script>